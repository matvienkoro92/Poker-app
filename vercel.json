{
  "headers": [
    {
      "source": "/index.html",
      "headers": [{ "key": "Cache-Control", "value": "no-store, max-age=0" }]
    }
  ],
  "functions": {
    "api/[[...slug]].js": { "maxDuration": 15 }
  },
  "crons": [
    {
      "path": "/api/cron-reminder-10min",
      "schedule": "21 21 * * 0"
    }
  ]
}
